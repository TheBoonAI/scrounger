<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest
  ExtensionBundleId="com.zorroa.scrounger"
  ExtensionBundleVersion="1.0.0"
  Version="10.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
  <ExtensionList>
    <Extension Id="com.zorroa.scrounger.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PPRO" Version="[9.0,99.9]" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="10.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.zorroa.scrounger.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Scrounger by Boon AI</Menu>
          <Geometry>
            <Size>
              <Height>800</Height>
              <Width>600</Width>
            </Size>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
